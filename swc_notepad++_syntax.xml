<NotepadPlus>
    <UserLang name="Mount&amp;Blade Syntax" ext="py">
        <Settings>
            <Global caseIgnored="yes" />
            <TreatAsSymbol comment="no" commentLine="yes" />
            <Prefix words1="no" words2="yes" words3="yes" words4="yes" />
        </Settings>
        <KeywordLists>
            <Keywords name="Delimiters">000000</Keywords>
            <Keywords name="Folder+">try_begin try_for_range try_for_range_backwards try_for_parties try_for_agents</Keywords>
            <Keywords name="Folder-">try_end</Keywords>
            <Keywords name="Operators">&quot; ( ) , [ ] |</Keywords>
            <Keywords name="Comment"> 1 1 2 2 0#</Keywords>
            <Keywords name="Words1">else_try</Keywords>
            <Keywords name="Words2">itm_ trp_ p_ script_ fac_ scn_ skl_ mt_ str_ mnu_ psys_ pt_ mesh_ psf_ arf_ blend_in_walk acf_</Keywords>
            <Keywords name="Words3">: $ reg</Keywords>
            <Keywords name="Words4">slot_</Keywords>
        </KeywordLists>
        <Styles>
            <WordsStyle name="DEFAULT" styleID="11" fgColor="FFFFFF" bgColor="222C28" fontName="" fontStyle="0" fontSize="10" />
            <WordsStyle name="FOLDEROPEN" styleID="12" fgColor="FF8000" bgColor="222C28" fontName="" fontStyle="0" fontSize="10" />
            <WordsStyle name="FOLDERCLOSE" styleID="13" fgColor="FF8000" bgColor="222C28" fontName="" fontStyle="0" />
            <WordsStyle name="KEYWORD1" styleID="5" fgColor="FF8000" bgColor="222C28" fontName="" fontStyle="0" />
            <WordsStyle name="KEYWORD2" styleID="6" fgColor="FDBF82" bgColor="222C28" fontName="Lucida Console" fontStyle="0" />
            <WordsStyle name="KEYWORD3" styleID="7" fgColor="80FF00" bgColor="222C28" fontName="Lucida Console" fontStyle="0" fontSize="9" />
            <WordsStyle name="KEYWORD4" styleID="8" fgColor="7A5C88" bgColor="222C28" fontName="Lao UI" fontStyle="0" />
            <WordsStyle name="COMMENT" styleID="1" fgColor="808080" bgColor="222C28" fontName="" fontStyle="0" />
            <WordsStyle name="COMMENT LINE" styleID="2" fgColor="808080" bgColor="222C28" fontName="" fontStyle="0" fontSize="10" />
            <WordsStyle name="NUMBER" styleID="4" fgColor="FF8040" bgColor="222C28" fontName="" fontStyle="0" />
            <WordsStyle name="OPERATOR" styleID="10" fgColor="989DD8" bgColor="222C28" fontName="" fontStyle="2" />
            <WordsStyle name="DELIMINER1" styleID="14" fgColor="808040" bgColor="222C28" fontName="Arial" fontStyle="2" fontSize="10" />
            <WordsStyle name="DELIMINER2" styleID="15" fgColor="808040" bgColor="222C28" fontName="Arial" fontStyle="0" />
            <WordsStyle name="DELIMINER3" styleID="16" fgColor="808040" bgColor="222C28" fontName="Arial" fontStyle="0" />
        </Styles>
    </UserLang>
</NotepadPlus>
